{
    "config": {
        "platform": {
            "php": "7.4.10"
        }
    },
    "require": {
        "php": "^7.4.10",
        "symfony/yaml": "^5.1",
        "doctrine/orm": "^2.12",
        "doctrine/migrations": "^3.5",
        "symfony/cache": "^5.3",
        "doctrine/annotations": "^1.13",
        "symfony/http-foundation": "^5.4",
        "symfony/dotenv": "^5.4",
        "ocramius/proxy-manager": "^2.13"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "App\\Acme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\Acme\\": "test/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
